Algoritmo "Consumo"

Var

   distancia, combustivel, consumo: real

Inicio

      escreval("Qual a distância percorrida pelo carro?")
      leia(distancia)
      escreval("Quantos litros de combustível o carro gastou?")
      leia(combustivel)
      
      consumo <- distancia / combustivel
      
      escreval("CONSUMO (Km/L): ", consumo)

Fimalgoritmo